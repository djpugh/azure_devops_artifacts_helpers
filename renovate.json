{
    "extends": [
        "config:base",
        ":preserveSemverRanges"
    ],
    "labels": ["chore"],
    "pip_requirements": {
        "fileMatch": ["embed_requirements.txt", "requirements.txt"]
    },
    "packageRules": [
        {
        "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
        "automerge": true,
        "automergeStrategy": "squash"
        }
    ]
}
